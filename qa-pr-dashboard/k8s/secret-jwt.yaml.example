# Example JWT Secret
# Copy this file to secret-jwt.yaml and update with your actual JWT secret
# DO NOT commit secret-jwt.yaml to git!
# Generate secret with: openssl rand -base64 32

apiVersion: v1
kind: Secret
metadata:
  name: jwt-secret
  namespace: qa-pr-dashboard
type: Opaque
stringData:
  jwt-secret: "your-strong-random-secret-key-here-change-this-in-production"
